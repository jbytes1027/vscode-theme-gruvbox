{
  "$schema": "vscode://schemas/color-theme",
  "name": "Gruvbox Dark Medium",
  "type": "dark",
  "tokenColors": [
    {
      "settings": {
        "foreground": "#ebdbb2"
      }
    },
    {
      "scope": "emphasis",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "strong",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "vars in comments",
      "scope": ["variable"],
      "settings": {
        "foreground": "#ebdbb2"
      }
    },
    {
      "name": "var related",
      "scope": [
        "storage.type", // ex int
        "storage.modifier", //ex static void
        "meta.function.decorator", // ex @python-decorator
        "entity.name.type.ts",
        "support.type.primitive.ts"
      ],
      "settings": {
        "foreground": "#8EC07C"
      }
    },
    {
      "scope": ["constant", "string", "keyword.control.anchor.regexp"],
      "settings": {
        "foreground": "#83A598"
      }
    },
    {
      "name": "package related and code flow",
      "scope": [
        "keyword.control",
        "keyword.other.package",
        "keyword.other.import"
      ],
      "settings": {
        "foreground": "#FB4934"
      }
    },
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "#a89984"
      }
    },
    {
      "name": "Selector",
      "scope": [
        "support.type.property-name.json", // JSON Selector/key
        "entity.name.tag" // HTML tag
      ],
      "settings": {
        "foreground": "#8EC07C"
      }
    },
    // MARKUP ----------------------------------------
    {
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.bold markup.italic",
      "settings": {
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "fix bold italic ***",
      "scope": "markup.bold punctuation.definition.italic",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "foreground": "#8ec07c",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#b8bb26"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#d65d0e"
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "#fe8019"
      }
    },
    // MARKDOWN
    {
      "scope": [
        "punctuation.definition.list.begin.markdown",
        "punctuation.definition.quote.begin.markdown"
      ],
      "settings": {
        "foreground": "#a89984",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "text.html.markdown punctuation.definition.link",
        "meta.link.inline.markdown"
      ],
      "settings": {
        "foreground": "#a89984"
      }
    },
    {
      "scope": [
        "markup.inline.raw",
        "markup.raw.block.markdown",
        "markup.fenced_code.block.markdown"
      ],
      "settings": {
        "foreground": "#d3869b"
      }
    }
  ],
  "colors": {
    // BASE COLORS
    "focusBorder": "#689d6a",
    "foreground": "#ebdbb2",
    "widget.shadow": "#1d2021",
    "selection.background": "#689d6a80",
